{
  "name": "livekit-dashboard",
  "version": "1.0.0",
  "description": "Open-source alternative to LiveKit Cloud Dashboard",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "npm run start:backend",
    "start:backend": "cd backend && npm run start",
    "preview:frontend": "cd frontend && npm run preview",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "clean": "rm -rf frontend/node_modules frontend/dist backend/node_modules backend/dist node_modules",
    "lint:frontend": "cd frontend && npm run lint",
    "type-check:frontend": "cd frontend && tsc --noEmit",
    "type-check:backend": "cd backend && tsc --noEmit"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  },
  "keywords": [
    "livekit",
    "dashboard",
    "webrtc",
    "real-time",
    "monitoring",
    "open-source"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/livekit-dashboard.git"
  }
}
